#include <stdio.h>

int main()
{
int n;
scanf("%d",&n);
int i;
int prime = 1;
for (i=2;i<n;i++){//进入循环验证n以内的数能否被小于n数整除
    if(n%i==0){//判断能否整除2，3等累加，能整除prime=0跳出进入下一个if
        prime = 0;
    }
}//判断prime的值
if(prime==1){//上面验证完成后进入是0就不是素数，是1则是
    printf("%dprime",n);
}else {
    printf("%d not prime",n);
}
return 0;
}
