#include <stdio.h>//了解指针地址变量和实际变量
void f(int *p);
void g(int k);
int main(void)
{
    int i =6;
    printf("&i=  %p\n",&i);
    f(&i);
    g(i); 
    return 0;
}
void f (int *p){
    printf("p=%p\n",p);//p是地址
    printf("*p=%d\n",*p);//*p是所指的int i的值
    *p = 26;//相当p直接对i进行修改赋值了
}
void g(int k){
    printf("k=%d\n",k);//没有指针 就是他inti的值
}
