//结构中的结构数组
#include <stdio.h>
struct point
{
    int x;
    int y;
};
struct rectangle
{
    struct point p1;//声明p1，类型point
    struct point p2 
};

void printRect(struct rectangle r)//声明函数
{
    printf("<%d,%d>to<%d,%d\n",r.p1.x,r.p1.y,r.p2.x,r.p2.y);
}
int main(int argc,char const *argv[])
{
    int i;
    struct rectangle rects[] ={{{1,2},{3,4}},{{5,6},{7,8}}};//0,1,2,3
    for(i=0;i<2;i++) printRect(rects[i]);
}
