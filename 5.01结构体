#include <stdio.h>
#include <string.h>
struct Books//结构体标签
{
    char title[50];
    char author[50];
    char subject[100];
    int book_id;//标准变量
};

void pirntBook(struct Books book);//声明函数
int main()
{
    struct Books Book1;//声明Book1类型为结构体标签
    struct Books Book2;
//book1详述
    strcpy(Book1.title,"C Programminhg");
    strcpy(Book1.author,"Nuha Ali");//名字不要起错
    strcpy(Book1.subject,"C Programminhg Tutorial");
    Book1.book_id = 123456;

    strcpy(Book2.title,"mach Programminhg");
    strcpy(Book2.author,"DONY");
    strcpy(Book2.subject,"math Programminhg");
    Book2.book_id =456789;
    printBook(Book1);
    printBook(Book2);//注意名字要一直print没有f
    return 0;
}
void printBook( struct Books book)
{
    printf( "Book title : %s\n", book.title);
   printf( "Book author : %s\n", book.author);
   printf( "Book subject : %s\n", book.subject);
   printf( "Book book_id : %d\n", book.book_id);
}
